<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
            <size
                android:height="3dp"
                android:width="3dp"/>
            <stroke
                android:width="1dp"
                android:color="#8692f7"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_ring_on"
        android:top="3dp"
        android:bottom="3dp"
        android:right="3dp"
        android:left="3dp">

    </item>

</layer-list>